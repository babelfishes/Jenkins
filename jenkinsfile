//Jenkinsfile example
//TODO: Jenkinsfile Methoden typisieren

node {
    checkout scm

    def classLoaderLoad = load 'src/main/groovy/de/se/jenkinsfile/ClassLoader_Script.groovy'
    classLoaderLoad()
    def classLoader = classLoaderLoad.createInstance()
    classLoader.loadClasses(classLoader,'.')
    classLoader.main.run("this is my main process")
}