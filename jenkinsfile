//Jenkinsfile example
//TODO: Jenkinsfile Methoden typisieren
node {
   checkout scm
   def functions = load 'src/functions.groovy'
   functions()
   def usit      = load 'src/usit.groovy'
   usit(functions.wrapper, functions.test1, functions.test2)
   usit.echoHelper()



   def wrapper = load 'src/de.se.jenkinsfile.groovy.PipelineWrapper.groovy'
   wrapper()
   wrapper.stage('hallo'){
        echo "hallo2"
   }

}